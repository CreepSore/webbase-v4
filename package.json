{
  "name": "webbase-v4",
  "version": "1.0.0",
  "description": "webbase v4 typescript",
  "main": "out/app.js",
  "scripts": {
    "build": "webpack build --env buildtype=app --mode=production && node ./src/buildWeb.js --mode=production",
    "dbuild": "webpack build --env buildtype=app --mode=development && node ./src/buildWeb.js --mode=development",
    "start": "npm run build && (clear || cls) && node ./out/app.js",
    "dstart": "npm run dbuild && (clear || cls) && DEBUG=true node ./out/app.js",
    "watch": "webpack build --env buildtype=app --mode=development -w",
    "watchWeb": "node ./src/buildWeb.js --mode=production -w",
    "dwatchWeb": "node ./src/buildWeb.js --mode=development -w",
    "generateDocs": "typedoc --out docs --basePath ./src --entryPointStrategy expand ./src",
    "lint": "eslint . --ext ts",
    "extinit": "node ./src/install.js --extinit",
    "test": "jest"
  },
  "author": "ehdes",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/express": "^5.0.5",
    "@types/express-session": "^1.18.2",
    "@types/express-ws": "^3.0.6",
    "@types/jest": "^30.0.0",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "babel": "^6.23.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "babel-preset-minify": "^0.5.2",
    "css-loader": "^7.1.2",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "tsconfig-paths-jest": "^0.0.1",
    "typedoc": "^0.28.14",
    "typedoc-plugin-rename-defaults": "^0.7.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/x-data-grid": "^8.20.0",
    "@tailwindcss/postcss": "^4.1.17",
    "cssnano": "^7.1.2",
    "esbuild-sass-plugin": "^3.3.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "express-ws": "^5.0.2",
    "helmet": "^8.1.0",
    "minimist": "^1.2.8",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0"
  }
