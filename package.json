{
  "name": "webbase-v4",
  "version": "1.0.0",
  "description": "webbase v4 typescript",
  "main": "out/app.js",
  "scripts": {
    "build": "webpack build --env buildtype=app --mode=production",
    "dbuild": "webpack build --env buildtype=app --mode=development",
    "start": "npm run build && clear && node ./out/app.js",
    "dstart": "npm run dbuild && clear && node ./out/app.js",
    "watch": "webpack build --env buildtype=app --mode=development -w"
  },
