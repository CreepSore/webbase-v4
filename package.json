{
  "name": "webbase-v4",
  "version": "1.0.0",
  "description": "webbase v4 typescript",
  "main": "out/app.js",
  "scripts": {
    "build": "webpack build --env buildtype=app --mode=production && webpack build --env buildtype=web --mode=production",
    "dbuild": "webpack build --env buildtype=app --mode=development && webpack build --env buildtype=web --mode=development",
    "start": "npm run build && (clear || cls) && node ./out/app.js",
    "dstart": "npm run dbuild && (clear || cls) && node ./out/app.js",
    "watch": "webpack build --env buildtype=app --mode=development -w",
    "watchWeb": "webpack build --env buildtype=web --mode=development -w"
  },
  "author": "ehdes",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/nodemailer": "^6.4.6",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "css-loader": "^6.7.1",
    "i": "^0.3.7",
    "npm": "^8.19.1",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "knex": "^2.3.0",
    "nodemailer": "^6.8.0",
    "sqlite3": "^5.0.11",
    "uuid": "^9.0.0"
  }
}
