  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "typescript": "^4.8.2",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}
