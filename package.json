{
  "name": "webbase-v4",
  "version": "1.0.0",
  "description": "webbase v4 typescript",
  "main": "out/app.js",
  "scripts": {
    "build": "webpack build --env buildtype=app --mode=production && webpack build --env buildtype=web --mode=production",
    "dbuild": "webpack build --env buildtype=app --mode=development && webpack build --env buildtype=web --mode=development",
    "start": "npm run build && (clear || cls) && node ./out/app.js",
    "dstart": "npm run dbuild && (clear || cls) && DEBUG=true node ./out/app.js",
    "watch": "webpack build --env buildtype=app --mode=development -w",
    "watchWeb": "webpack build --env buildtype=web --mode=development -w",
    "pwatchWeb": "webpack build --env buildtype=web --mode=production -w",
    "generateDocs": "typedoc --out docs --basePath ./src --entryPointStrategy expand ./src",
    "lint": "eslint . --ext ts",
    "log": "cat $(ls -t logs/* | head -1) | vim - -c 'set filetype=messages'",
    "jsonlog": "cat $(ls -t jsonlogs/* | head -1) | vim - -c 'set filetype=messages'"
  },
  "author": "ehdes",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/express-ws": "^3.0.1",
    "@types/jest": "^29.2.4",
    "@types/minimist": "^1.2.2",
    "@types/nodemailer": "^6.4.6",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel": "^6.23.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "babel-preset-minify": "^0.5.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "jest": "^29.3.1",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.25",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.8.2",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-ws": "^5.0.2",
    "graphql": "^16.6.0",
    "graphql-http": "^1.16.0",
    "graphql-tools": "^8.3.18",
    "helmet": "^6.0.0",
    "knex": "^2.3.0",
    "mime": "^3.0.0",
    "minimist": "^1.2.8",
    "nodemailer": "^6.8.0",
    "sqlite3": "^5.0.11",
    "uuid": "^9.0.0"
  }
}
